{
  "name": "swagger-typescript",
  "version": "4.0.12",
  "description": "A auto generate typescript code from swagger",
  "main": "bin/index.js",
  "bin": {
    "swag-ts": "bin/index.js"
  },
  "scripts": {
    "generate": "yarn test",
    "prepare": "rm -rf ./lib && tsc",
    "test": "yarn prepare && node ./lib/index.js",
    "lint": "eslint src/**/*.ts",
    "eslint-fix": "eslint src/**/*.ts --fix",
    "prettier-fix": "prettier --write \"**/*.{ts,tsx}\"",
    "changeLog": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hosseinmd/swagger-typescript.git"
  },
  "keywords": [
    "swagger",
    "typescript",
    "nswag",
    "openApi 3",
    "code generator"
  ],
  "author": "Hossein mohammadi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hosseinmd/swagger-typescript/issues"
  },
  "homepage": "https://github.com/hosseinmd/swagger-typescript#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/generator": "^7.11.6",
    "@babel/plugin-transform-typescript": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/react-hooks": "3.2.1",
    "@testing-library/react-native": "5.0.3",
    "@types/jest": "25.2.3",
    "@types/qs": "^6.9.5",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^2.20.0",
    "babel-jest": "^24.9.0",
    "babel-preset-react-native": "^4.0.1",
    "commitlint": "^8.3.5",
    "eslint": "^7.8.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.3",
    "jest": "26.0.1",
    "prettier-plugin-jsdoc": "^0.2.4",
    "standard-version": "^9.0.0",
    "tsc-prog": "^2.2.1",
    "ts-jest": "25.5.1",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint"
    }
  },
  "files": [
    "lib",
    "files"
  ]
}
